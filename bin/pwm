#!/usr/bin/env node

require('commander')
  .version(require('../package.json').version)
  .command('install', 'install module/s by name or from module.json')
  .command('uninstall', 'uninstall module/s by name')
  .command('search', 'search for modules by name')
  .command('fetch', 'update the module search cache')
  .parse(process.argv)
